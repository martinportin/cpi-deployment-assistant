{
  "name": "SAP CPI Deploy Assistant",
  "description": "Chrome extension for deploying and undeploying SAP CPI artifacts.",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "index.html",
    "default_title": "SAP CPI Deploy Assistant"
  },
  "content_scripts": [
    {
      "matches": [
        "https://*.cfapps.eu20.hana.ondemand.com/shell/*"
      ],
      "js": [
        "./static/js/content.js"
      ]
    }
  ]
}